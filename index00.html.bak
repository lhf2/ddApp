	//当打开找驾校页面默认鉴权
	function zjxForAuoth2() {
				//看用户是否使用过,保存的缓存信息
				var jsonStrUser=localStorage.getItem("wx_user");
				if(utils.isEmpty(jsonStrUser)){//不存在去授权
					//console.log('对象不存在去授权');
					var backUri = "http://wx.dingdongxueche.com/wx2pay/userAuth/zjxInfo";
					//鉴权获取用户信息
					window.location.href= "https://open.weixin.qq.com/connect/oauth2/authorize?"
						+ "appid=wx9766e4b2a8cb0844&redirect_uri="
						+ encodeURIComponent(backUri)
						+ "&response_type=code&scope=snsapi_userinfo&state=STATE#wechat_redirect";
				}else{
				 	//console.log('对象存在去中心');
					var jsonUser = JSON.parse(jsonStrUser);
					localStorage.setItem("openId",jsonUser.openId);
